{
  "name": "alessa-ordering",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${PORT:-3001} -H ${NEXTHOST:-127.0.0.1}",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:seed": "node prisma/seed.js",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "clean": "rm -rf .next node_modules/.cache",
    "setup": "npm install && npm run db:generate",
    "dev:clean": "npm run clean && npm run dev",
    "build:clean": "npm run clean && npm run build",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:smoke": "node -e \"try{require('fs').rmSync('.smoke-tests',{recursive:true,force:true});}catch{}\" && tsc --project tsconfig.smoke.json && node .smoke-tests/tests/smoke/index.js",
    "verify": "npm run lint && npm run test:types && npm run test:smoke",
    "verify:full": "bash scripts/verify.sh",
    "seed:lasreinas": "node scripts/seed-lasreinas.mjs",
    "cap:sync": "npx cap sync",
    "cap:ios": "npx cap open ios",
    "build:ios": "npm run build && npx cap sync ios",
    "deploy:ipad": "bash scripts/deploy-to-ipad.sh",
    "xcode:diagnose": "bash scripts/xcode-deploy-diagnostic.sh",
    "xcode:fix": "bash scripts/xcode-deploy-fix.sh",
    "ipad:check": "bash scripts/check-ipad-connection.sh",
    "ipad:wireless": "bash scripts/setup-wireless-deployment.sh",
    "ipad:setup": "bash scripts/setup-wireless-now.sh",
    "test:stripe": "tsx scripts/test-stripe-config.ts",
    "generate:onboarding": "tsx scripts/generate-onboarding-link.ts",
    "check:account-mode": "tsx scripts/check-stripe-account-mode.ts",
    "reset:stripe-account": "tsx scripts/reset-stripe-account.ts",
    "verify:stripe-mode": "tsx scripts/verify-stripe-mode.ts"
  },
  "dependencies": {
    "@capacitor-community/bluetooth-le": "^7.3.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/splash-screen": "^6.0.4",
    "@prisma/client": "^5.22.0",
    "@stripe/react-stripe-js": "^2.9.0",
    "@stripe/stripe-js": "^4.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.4",
    "bcryptjs": "^3.0.3",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.552.0",
    "next": "14.0.3",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
    "react": "^18.2.0",
    "react-d3-tree": "^3.6.6",
    "react-dom": "^18.2.0",
    "recharts": "^3.5.1",
    "stripe": "^20.0.0",
    "tailwindcss": "^3.4.18",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "puppeteer": "^24.31.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2"
  }
}
